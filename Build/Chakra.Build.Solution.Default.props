<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- ========= Microsoft.Build.Paths ========= -->
    <SdkIncPath>$(PUBLIC_ROOT)\sdk\inc</SdkIncPath>
    <SdkLibPath>$(PUBLIC_ROOT)\sdk\lib\$(PlatformPathNameAlt)</SdkLibPath>
    <CrtIncPath>$(SdkIncPath)\crt</CrtIncPath>
    <MinCoreSdkLibPath>$(PUBLIC_ROOT)\sdk\lib\mincore\$(PlatformPathNameAlt)</MinCoreSdkLibPath>
    <OneCoreSdkLibPath>$(PUBLIC_ROOT)\sdk\lib\onecore\$(PlatformPathNameAlt)</OneCoreSdkLibPath>
    <OneCorePrivSdkLibPath>$(PUBLIC_ROOT)\internal\onecore\priv_sdk\lib\$(PlatformPathNameAlt)</OneCorePrivSdkLibPath>

    <PublicInternalPath>$(PUBLIC_ROOT)\internal</PublicInternalPath>
    <InternalSdkIncPath>$(PublicInternalPath)\sdk\inc</InternalSdkIncPath>
    <InternalSdkLibPath>$(PublicInternalPath)\sdk\lib\$(PlatformPathNameAlt)</InternalSdkLibPath>
    <ComIncPath>$(PublicInternalPath)\com\inc</ComIncPath>
    <MinWinPrivSdkIncPath>$(PublicInternalPath)\minwin\priv_sdk\inc</MinWinPrivSdkIncPath>
    <MinWinPrivSdkLibPath>$(PublicInternalPath)\minwin\priv_sdk\lib\$(PlatformPathNameAlt)</MinWinPrivSdkLibPath>
    <MinCorePrivSdkLibPath>$(PublicInternalPath)\mincore\priv_sdk\lib\$(PlatformPathNameAlt)</MinCorePrivSdkLibPath>
    <BaseLibPath Condition="'$(BaseLibPath)'==''">$(PublicInternalPath)\base\lib\$(PlatformPathNameAlt)</BaseLibPath>

    <OneCoreBaseLibDest Condition="'$(OneCoreBaseLibDest)' == ''">$(PublicInternalPath)\onecorebase\lib</OneCoreBaseLibDest>
    <OneCoreBaseLibPath Condition="'$(OneCoreBaseLibPath)' == ''">$(OneCoreBaseLibDest)\$(PlatformPathNameAlt)</OneCoreBaseLibPath>
    <OneCoreComIncPath Condition="'$(OneCoreComIncPath)' == ''">$(PublicInternalPath)\onecorecom\inc</OneCoreComIncPath>
    <OneCoreInetcoreIncPath Condition="'$(OneCoreInetcoreIncPath)' == ''">$(PublicInternalPath)\onecoreinetcore\inc</OneCoreInetcoreIncPath>
    <OneCoreSdkToolsIncPath Condition="'$(OneCoreSdkToolsIncPath)' == ''">$(PublicInternalPath)\onecoresdktools\inc</OneCoreSdkToolsIncPath>
    <OneCoreShellLibDest Condition="'$(OneCoreShellLibDest)' == ''">$(PublicInternalPath)\onecoreshell\lib</OneCoreShellLibDest>
    <OneCoreShellLibPath Condition="'$(OneCoreShellLibPath)' == ''">$(OneCoreShellLibDest)\$(PlatformPathNameAlt)</OneCoreShellLibPath>
    <OneCoreUapComIncPath Condition="'$(OneCoreUapComIncPath)' == ''">$(PublicInternalPath)\onecoreuapcom\inc</OneCoreUapComIncPath>
    <OneCoreUapInetcoreLibDest Condition="'$(OneCoreUapInetcoreLibDest)' == ''">$(PublicInternalPath)\onecoreuapinetcore\lib</OneCoreUapInetcoreLibDest>
    <OneCoreUapInetcoreLibPath Condition="'$(OneCoreUapInetcoreLibPath)' == ''">$(OneCoreUapInetcoreLibDest)\$(PlatformPathNameAlt)</OneCoreUapInetcoreLibPath>
    <OneCoreUapShellIncPath Condition="'$(OneCoreUapShellIncPath)' == ''">$(PublicInternalPath)\onecoreuapshell\inc</OneCoreUapShellIncPath>
    <OneCoreUapShellLibDest Condition="'$(OneCoreUapShellLibDest)' == ''">$(PublicInternalPath)\onecoreuapshell\lib</OneCoreUapShellLibDest>
    <OneCoreUapShellLibPath Condition="'$(OneCoreUapShellLibPath)' == ''">$(OneCoreUapShellLibDest)\$(PlatformPathNameAlt)</OneCoreUapShellLibPath>

    <BaseDataPath>$(RazzleDataPath)</BaseDataPath>

    <SdkToolsIncPath>$(PublicInternalPath)\sdktools\inc</SdkToolsIncPath>
  </PropertyGroup>
  <PropertyGroup>
    <RazzleConfigurationPath Condition="'$(Configuration)'=='Debug'">chk</RazzleConfigurationPath>
    <RazzleConfigurationPath Condition="'$(Configuration)'!='Debug'">fre</RazzleConfigurationPath>
    <PUBLIC_ROOT Condition="'$(PUBLIC_ROOT)'==''">$(SolutionDir)..\..\..\sdpublic</PUBLIC_ROOT>
    <OSBUILDTOOLSROOT Condition="'$(OSBUILDTOOLSROOT)'==''">$(RazzleToolPath)</OSBUILDTOOLSROOT>

    <RazzleToolPath Condition="'$(RazzleToolPath)'==''">$(SolutionDir)..\..\..\tools</RazzleToolPath>
    <IncludePath>$(PUBLIC_ROOT)\shared\inc</IncludePath>
    <IncludePath>$(IncludePath);$(PUBLIC_ROOT)\shared\inc\minwin</IncludePath>
    <IncludePath>$(IncludePath);$(SdkIncPath)</IncludePath>
    <IncludePath>$(IncludePath);$(SdkIncPath)\minwin</IncludePath>
    <IncludePath>$(IncludePath);$(SdkIncPath)\mincore</IncludePath>
    <IncludePath>$(IncludePath);$(SolutionDir)..\..\inc</IncludePath>
       
    <ExecutablePath>$(OSBUILDTOOLSROOT)\vc\HostX86\$(PlatformPathName);$(RazzleToolPath)\x86</ExecutablePath>
    <ExecutablePath Condition="'$(Platform)'=='x64' and '$(BuildingInsideVisualStudio)'!='true'">$(OSBUILDTOOLSROOT)\vc\HostX64\amd64;$(RazzleToolPath)\amd64</ExecutablePath>
    <ExecutablePath Condition="'$(Platform)'=='x64' and '$(BuildingInsideVisualStudio)'=='true'">$(OSBUILDTOOLSROOT)\vc\HostX86\amd64;$(RazzleToolPath)\x86</ExecutablePath>
   
    <GenerateManifest>false</GenerateManifest>
    <!-- disable incremental build because some library from public contains /GL objects -->
    <LinkIncremental>false</LinkIncremental>
    <!-- Minimal Rebuild /Gm write to this file that we don't really depend on -->
    <NoDependencies>%(NoDependencies);$(IntDir)vc100.idb;$(IntDir)vc100.pdb;$(IntDir)vc110.idb;$(IntDir)vc110.pdb;$(IntDir)vc120.pdb;$(IntDir)vc120.idb</NoDependencies>
    <InstrumentationManifestIncludeDirectory Condition="'$(InstrumentationManifestIncludeDirectory)'==''">$(IntDir)..\CoreManifests\</InstrumentationManifestIncludeDirectory>
    <ChakraManifestsIncludeDirectory>$(IntDir)..\manifests</ChakraManifestsIncludeDirectory>
    <JscriptInfoIntDir>$(IntDir)..\Chakra.Engine</JscriptInfoIntDir>
    <ChakraInternalIDLIntDir>$(IntDir)..\Chakra.InternalIDL</ChakraInternalIDLIntDir>
    <ChakraJITIDLIntDir>$(IntDir)..\Chakra.JITIDL</ChakraJITIDLIntDir>
    <ChakraPublishIntDir>$(IntDir)..\Publish</ChakraPublishIntDir>

    <ChakraCommonMemoryLib>$(IntDir)..\Chakra.Common.Memory\Chakra.Common.Memory.lib</ChakraCommonMemoryLib>

    <!-- ========= From makefile.def ========= -->
    <LinkerAppVersion Condition="'$(LinkerAppVersion)' == ''">10.0</LinkerAppVersion>
    <LinkerOSVersion Condition="'$(LinkerOSVersion)' == ''">10.0</LinkerOSVersion>
  </PropertyGroup>

</Project>