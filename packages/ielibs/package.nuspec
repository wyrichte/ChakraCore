<?xml version="1.0"?>
<package >
  <metadata>
    <id>IELibs</id>
    <version>1.0.4</version>
    <authors>hiteshk</authors>
    <owners>hiteshk</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>IE Libs Package</description>
    <releaseNotes>IE-owned libs that Chakra depends on to build</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>Chakra IE Libs</tags>
  </metadata>
  <files>
    <!-- These files are relative to inetcore\jscript\packages\ielibs\stage -->
    <file src="..\..\..\..\inc\devtbhost.h" target="devtbhost.h" />
    <file src="..\..\..\..\inc\ScriptProjectionHost.hxx" target="ScriptProjectionHost.hxx" />

    <!-- pdm files needed for debugger tests (these files are also relative to \stage) -->
    <file src="..\..\..\..\devtoolbar\jstools\setup\x86\*" target="pdm\x86" />
    <file src="..\..\..\..\devtoolbar\jstools\setup\amd64\*" target="pdm\x64" />
    <file src="..\..\..\..\devtoolbar\jstools\setup\arm\*" target="pdm\arm" />

    <!-- These two are entire directories that need to be copied -->
    <file src="manifests\**" target="manifests" />
    <file src="lib\**" />
  </files>
</package>
