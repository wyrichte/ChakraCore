<?xml version="1.0" encoding="utf-8"?>
<regress-exe>
  <test>
    <default>
      <files>math.js</files>
      <!-- x86 all -->
      <baseline>math.baseline</baseline>
      <tags>exclude_amd64,exclude_arm</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.x64.baseline</baseline>
      <!-- Win7 x64 -->
      <tags>exclude_arm,exclude_x86,exclude_win8,exclude_winBlue,exclude_win10,Slow,Slow</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.x64.win8.baseline</baseline>
      <!-- Win8/Blue x64 -->
      <tags>exclude_arm,exclude_x86,exclude_win7,Slow,Slow</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.arm.baseline</baseline>
      <!--  ARM -->
      <compile-flags>-testEtwDll:NativeUnitTests.dll</compile-flags>
      <tags>exclude_x86,exclude_amd64</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <!-- x86 all OS -->
      <baseline>math.baseline</baseline>
      <compile-flags>-testEtwDll:NativeUnitTests.dll</compile-flags>
      <tags>exclude_amd64,exclude_arm</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.x64.baseline</baseline>
      <!--  Win7 x64 -->
      <compile-flags>-testEtwDll:NativeUnitTests.dll</compile-flags>
      <tags>exclude_arm,exclude_x86,exclude_win8,exclude_winBlue,exclude_win10,Slow,Slow</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.x64.win8.baseline</baseline>
      <!--  Win8/blue x64 -->
      <compile-flags>-testEtwDll:NativeUnitTests.dll</compile-flags>
      <tags>exclude_arm,exclude_x86,exclude_win7,Slow,Slow</tags>
    </default>
  </test>
  <test>
    <default>
      <files>math.js</files>
      <baseline>math.arm.baseline</baseline>
      <!--  ARM -->
      <compile-flags>-testEtwDll:NativeUnitTests.dll</compile-flags>
      <tags>exclude_x86,exclude_amd64</tags>
    </default>
  </test>
</regress-exe>
