        $(IntDir);
        $(ChakraRootDirectory)core\lib\common;
        $(ChakraRootDirectory)private\lib\JsrtChakra;
        $(ChakraRootDirectory)core\lib\jsrt;
        $(ChakraRootDirectory)private\lib\engine;

        <!-- For EdgeScriptDirect.h and EdgeJavascriptTypeId.h -->
        $(ChakraPublishIntDir);
        $(ChakraCoreRootDirectory)lib\runtime\types;

        %(AdditionalIncludeDirectories)
      </AdditionalIncludeDirectories>
      <PrecompiledHeader>Use</PrecompiledHeader>
      <PrecompiledHeaderFile>HostCommonPch.h</PrecompiledHeaderFile>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <ClCompile Include="$(MSBuildThisFileDirectory)debuggercontroller.cpp" />
    <ClCompile Include="$(MSBuildThisFileDirectory)HostConfigFlags.cpp" />
    <ClCompile Include="$(MSBuildThisFileDirectory)HostSysInfo.cpp" />
    <ClCompile Include="$(MSBuildThisFileDirectory)JScript9Interface.cpp" />
