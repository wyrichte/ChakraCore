    </ClCompile>
    <ClCompile Include="$(MSBuildThisFileDirectory)TestLargeAddress.cpp" />
  </ItemGroup>
  <ItemGroup>
    <ClInclude Include="debuggercontroller.h" />
    <ClInclude Include="HostConfigFlags.h" />
    <ClInclude Include="HostSysInfo.h" />
    <ClInclude Include="HostConfigFlagsList.h" />
    <ClInclude Include="JScript9Interface.h" />
    <ClInclude Include="HostCommonPch.h" />
    <ClInclude Include="TestLargeAddress.h" />
  </ItemGroup>
  <ItemGroup>
    <CustomBuild Include="dbgcontroller.js">
