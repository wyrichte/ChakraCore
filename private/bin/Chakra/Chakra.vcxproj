        api-ms-win-core-winrt-l1-*.dll;
        api-ms-win-ro-typeresolution-l1-*.dll
      </DelayLoadDLLs>
    </Link>
    <ImpLib>
      <AdditionalDependencies>$(ChakraLibs);%(AdditionalDependencies)</AdditionalDependencies>
    </ImpLib>
    <PostBuildEvent>
